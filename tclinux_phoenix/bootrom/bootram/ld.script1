/*OUTPUT_FORMAT(elf32-tradbigmips)
OUTPUT_ARCH(mips)*/
ENTRY(start)
SECTIONS
{
  /* Read-only sections, merged into text segment: */
  . = 0x807b0000;
  .init          : { *(.init)		} =0
  .text      :
  {
    _ftext = . ;
    *(.text)
    *(.rodata)
    *(.rodata1)
    /* .gnu.warning sections are handled specially by elf32.em.  */
    *(.gnu.warning)
  } =0
  
  _etext = .;

  

 
  . = .;
  .data    :
  {
    _fdata = . ;
    *(.data)

   

    CONSTRUCTORS
     _edata  =  .;
  PROVIDE (edata = .);

  }
  
  
  
  __bss_start = .;
  _fbss = .;
  .sbss      : { *(.sbss) *(.scommon) }
  .bss       :
  {
   *(.dynbss)
   *(.bss)
   *(COMMON)
   .  = ALIGN(4);
  _end = . ;
  PROVIDE (end = .);
  }=0

  
}
