	cp $(TRUNK_DIR)/rootfs_slave $(APP_TCLINUXBUILDER_DIR)
	rm -f $(TRUNK_DIR)/rootfs_slave
	rm -rf $(TRUNK_DIR)/tmpfilesys_slave
