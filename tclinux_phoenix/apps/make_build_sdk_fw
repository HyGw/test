	cp -rf $(TRUNK_DIR)/tmpfilesys_slave/filesystem/boaroot/cgi-bin/net-wanset.asp $(TRUNK_DIR)/tmpfilesys_slave/filesystem/boaroot/cgi-bin_tmp
