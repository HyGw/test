	$(STRIP) --strip-unneeded -R .comment  $(TRUNK_DIR)/tmpfilesys_slave/filesystem/usr/bin/*
	$(STRIP) --strip-unneeded -R .comment  $(TRUNK_DIR)/tmpfilesys_slave/filesystem/userfs/bin/*
