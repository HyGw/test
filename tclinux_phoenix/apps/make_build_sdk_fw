ifneq ($(strip $(TCSUPPORT_2_6_36_KERNEL) $(TCSUPPORT_3_18_21_KERNEL)),)
	$(TOOLS_DIR)/mksquashfs4 $(TRUNK_DIR)/tmpfilesys_slave/filesystem $(TRUNK_DIR)/rootfs_slave -comp lzma -nopad -noappend
