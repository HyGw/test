ifneq ($(strip $(TCSUPPORT_CT_PON_CY) $(TCSUPPORT_CT_PROLINE_SUPPORT)),)
	cp $(FILESYSTEM_DIR)/userfs/ctromfile_f.cfg $(APP_TCLINUXBUILDER_DIR)
