## Axel settings, generated by configure
PREFIX=/usr/local
BINDIR=/usr/local/bin
ETCDIR=/usr/local/etc
SHAREDIR=/usr/local/share
MANDIR=/usr/local/share/man
LOCALE=/usr/local/share/locale

OUTFILE=axel
ARCH=Linux

DESTDIR=
LFLAGS=

CFLAGS=-g -O2 -Wall -mips32r2 -muclibc

ifneq ($(TC_CFLAGS),)
CFLAGS+=$(TC_CFLAGS)
endif

ifneq ($(strip $(TCSUPPORT_NEWTOOLCHAIN)),)
CC=mips-unknown-linux-uclibc-gcc
STRIP=mips-unknown-linux-uclibc-strip
endif
LFLAGS+=-lpthread
