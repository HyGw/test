SEDCMD="$SEDCMD -e 's/# USING_CROSS_COMPILER is not set/USING_CROSS_COMPILER=y/'"

#Busybox IPv6 config change script
if [ "$TCSUPPORT_IPV6" == "" ] ;then
SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_IPV6=y/# CONFIG_FEATURE_IPV6 is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_PING6=y/# CONFIG_PING6 is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_FANCY_PING6=y/# CONFIG_FEATURE_FANCY_PING6 is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_SYSCTL=y/# CONFIG_SYSCTL is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_IP_NEIGH=y/# CONFIG_FEATURE_IP_NEIGH is not set/'"
#else
#SEDCMD="$SEDCMD -e 's/CONFIG_IP=y/# CONFIG_IP is not set/'"
#SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_IP_ADDRESS=y/# CONFIG_FEATURE_IP_ADDRESS is not set/'"
#SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_IP_LINK=y/# CONFIG_FEATURE_IP_LINK is not set/'"
#SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_IP_ROUTE=y/# CONFIG_FEATURE_IP_ROUTE is not set/'"
#SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_IP_TUNNEL=y/# CONFIG_FEATURE_IP_TUNNEL is not set/'"
fi

#CPU type config change script
if [ "$TCSUPPORT_CPU_TC3162U" == "" ] ;then
SEDCMD="$SEDCMD -e 's/CONFIG_UMOUNT=y/# CONFIG_UMOUNT is not set/'"
fi

if [ "$TCSUPPORT_CPU_TC3262" == "" ] ;then
SEDCMD="$SEDCMD -e 's/CONFIG_GUNZIP=y/# CONFIG_GUNZIP is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_GZIP=y/# CONFIG_GZIP is not set/'"
else
SEDCMD="$SEDCMD -e 's/CROSS_COMPILER_PREFIX=\"mips-linux-\"/CROSS_COMPILER_PREFIX=\"mips-linux-uclibc-\"/'"
SEDCMD="$SEDCMD -e 's/EXTRA_CFLAGS_OPTIONS=\"-mips1 -msoft-float\"/EXTRA_CFLAGS_OPTIONS=\"-msoft-float -mips32r2\"/'"
fi

if [ "$TCSUPPORT_CPU_TC3182" == "" ] ;then
SEDCMD="$SEDCMD -e 's/CONFIG_GUNZIP=y/# CONFIG_GUNZIP is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_GZIP=y/# CONFIG_GZIP is not set/'"
else
SEDCMD="$SEDCMD -e 's/CROSS_COMPILER_PREFIX=\"mips-linux-\"/CROSS_COMPILER_PREFIX=\"mips-linux-uclibc-\"/'"
SEDCMD="$SEDCMD -e 's/EXTRA_CFLAGS_OPTIONS=\"-mips1 -msoft-float\"/EXTRA_CFLAGS_OPTIONS=\"-msoft-float -mips32r2\"/'"
fi

#Pure bridge config change script
if [ "$TCSUPPORT_PUREBRIDGE" != "" ] ;then
SEDCMD="$SEDCMD -e 's/CONFIG_GUNZIP=y/# CONFIG_GUNZIP is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_GZIP=y/# CONFIG_GZIP is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_TAR=y/# CONFIG_TAR is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_TAR_CREATE=y/# CONFIG_FEATURE_TAR_CREATE is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_TAR_GZIP=y/# CONFIG_FEATURE_TAR_GZIP is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_BASENAME=y/# CONFIG_BASENAME is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_CUT=y/# CONFIG_CUT is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_DIRNAME=y/# CONFIG_DIRNAME is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_ENV=y/# CONFIG_ENV is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_EXPR=y/# CONFIG_EXPR is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_FALSE=y/# CONFIG_FALSE is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_HEAD=y/# CONFIG_HEAD is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_MV=y/# CONFIG_MV is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_PWD=y/# CONFIG_PWD is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_RMDIR=y/# CONFIG_RMDIR is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_TRUE=y/# CONFIG_TRUE is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_UNAME=y/# CONFIG_UNAME is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_YES=y/# CONFIG_YES is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_IP=y/# CONFIG_IP is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_IP_ADDRESS=y/# CONFIG_FEATURE_IP_ADDRESS is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_IP_LINK=y/# CONFIG_FEATURE_IP_LINK is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_IP_ROUTE=y/# CONFIG_FEATURE_IP_ROUTE is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_IP_TUNNEL=y/# CONFIG_FEATURE_IP_TUNNEL is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_NETSTAT=y/# CONFIG_NETSTAT is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_NSLOOKUP=y/# CONFIG_NSLOOKUP is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_PING=y/# CONFIG_PING is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_FANCY_PING=y/# CONFIG_FEATURE_FANCY_PING is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_ROUTE=y/# CONFIG_ROUTE is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_TFTP=y/# CONFIG_TFTP is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_TFTP_GET=y/# CONFIG_FEATURE_TFTP_GET is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_TFTP_PUT=y/# CONFIG_FEATURE_TFTP_PUT is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_UDHCPD=y/# CONFIG_UDHCPD is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_UDHCPC=y/# CONFIG_UDHCPC is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_TOP=y/# CONFIG_TOP is not set/'"
SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_COMMAND_HISTORY=8/CONFIG_FEATURE_COMMAND_HISTORY=15/'"
SEDCMD="$SEDCMD -e 's/CONFIG_FEATURE_COMMAND_TAB_COMPLETION=y/# CONFIG_FEATURE_COMMAND_TAB_COMPLETION is not set/'"
fi

gen="sed $SEDCMD busyboxtemplate.config"
eval $gen > busybox.config
